# System utils

function system.findappbundleid() {
	osascript -e "id of app \"$1\""
	#echo 'id of app' "$1" 'is something'
	#echo "id of app $@"
}

function system.getcurrentdate() {
  local curdate=$(date +'%Y-%m-%d-%T')
  curdate="${curdate//:/-}"
  echo $curdate
}

function system.findfolder() {
	mdfind kind:folder "$1"
}
